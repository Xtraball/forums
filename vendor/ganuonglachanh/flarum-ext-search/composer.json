{
    "name": "ganuonglachanh/flarum-ext-search",
    "description": "Allows to search by discusstion title",
    "keywords": ["search","flarum"],
    "type": "flarum-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "GaNuongLaChanh",
            "email": "ganuonglachanh@outlook.com"
        }
    ],
    "support": {
        "issues": "https://github.com/ganuonglachanh/flarum-ganuonglachanh-search/issues",
        "source": "https://github.com/ganuonglachanh/flarum-ganuonglachanh-search"
    },
    "require": {
        "flarum/core": "^0.1.0-beta.6"
    },
    "extra": {
